<div class='container px-5 px-sm-0'>
<h2>Books seach for</h2>
<table class="table">
  <% if @range == "User" %>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.name %></td>
        </tr>
      <% end %>
    </tbody>
  <% else %>
    <tboyd>
     <% @books.each do |book| %>
      <tr>
       <td><%= book.title %></td>
       <td><%= book.body %></td>
      </tr>
     <% end %>
    </tbody>
  <% end %>
</table>